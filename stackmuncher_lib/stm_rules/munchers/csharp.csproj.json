{
  "$schema": "https://raw.githubusercontent.com/stackmuncher/stm_app/master/stackmuncher_lib/stm_rules/schemas/muncher.json",
  "line_endings": "unix",
  "language": "C#",
  "packages": [
    "(?i)sdk\\s*=\\s*\"([.[^\"]]+)\"",
    "(?i)<TargetFramework>([.[^<]]+)<",
    "(?i)<PackageReference.+Include=\"([.[^\"]]+)\".+Version=\"([.[^\"]]+)\"",
    "(?i)<(TargetFrameworkVersion)>([.[^<]]+)<",
    "(?i)<Reference[.\\s[^>]]+Include=\"([.[^\"]]+)\""
  ]
}