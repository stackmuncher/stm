{
  "$schema": "https://raw.githubusercontent.com/stackmuncher/stm_app/master/stackmuncher_lib/stm_rules/schemas/muncher.json",
  "line_endings": "unix",
  "language": "Jupyter",
  "bracket_only": [
    "^[[:blank:]]*[{}\\[\\]\\(\\)][[:blank:];,)]*$"
  ],
  "refs": [
    "^\\s*\"\\s*import\\s+([A-Za-z0-9_\\.]+)",
    "^\\s*\"\\s*from\\s+([A-Za-z0-9_\\.]+)\\s+import\\s+([A-Za-z0-9_\\.]+)"
  ]
}